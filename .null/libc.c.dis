#ifndef TLIBC
#define TLIBC

#include "inc.h"

char* strncpy(char* dest, const char* src, size_t n) {
    char* original_dest = dest;
    size_t i;
    for (i = 0; i < n && src[i] != '\0'; i++) {
        dest[i] = src[i];
    }
    for (; i < n; i++) {
        dest[i] = '\0';
    }

    return original_dest;
}

#endif